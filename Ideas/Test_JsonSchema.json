{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Ofen Daten",
  "type": "object",
  "properties": {
    "Temperatur": {
      "type": "integer",
      "description": "Die Temperatur des Ofens in Grad Celsius",
      "minimum": 0,
      "maximum": 300
    },
    "Modus": {
      "type": "string",
      "description": "Der Betriebsmodus des Ofens",
      "enum": ["Umluft", "Ober-/Unterhitze", "Grillen"]
    },
    "Timer": {
      "type": "integer",
      "description": "Timer in Minuten",
      "minimum": 0
    },
    "IstEingeschaltet": {
      "type": "boolean",
      "description": "Gibt an, ob der Ofen eingeschaltet ist"
    }
    
  }

